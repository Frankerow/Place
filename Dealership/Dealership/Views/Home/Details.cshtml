@model IEnumerable<Dealership.Models.TechnicalDataVM>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BodyType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numbersofdoors)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numbersofplaces)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EugineType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EugineLocation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EugineVolume)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BodyType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numbersofdoors)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numbersofplaces)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EugineType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EugineLocation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EugineVolume)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
